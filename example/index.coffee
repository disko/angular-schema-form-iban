app = angular.module('app', [ 'schemaForm' ])

app.controller 'formCtrl', ($scope) ->

  $scope.schema =
    'type': 'object'
    'title': 'Comment'
    'properties':
      'iban1':
        'title': 'IBAN 1 (type string)'
        'type': 'string'
      'iban2':
        'title': 'IBAN 2 (type iban)'
        'type': 'iban'
    'required': ['iban1', 'iban2']

  $scope.form = [
    {
      key: 'iban1'
      type: 'iban'
    }
    { key: 'iban2' }
  ]

  $scope.model = {}

  $scope.$watch 'model', ((value) ->
    if value
      $scope.prettyModel = JSON.stringify(value, undefined, 2)
    return
  ), true

  $scope.setValidIban = ->
    $scope.model.iban1 = validIbans[Math.floor(Math.random() * validIbans.length)]

  $scope.setInvalidIban = ->
    $scope.model.iban1 = '123'

  return
